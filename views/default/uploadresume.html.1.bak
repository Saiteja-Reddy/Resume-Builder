{{extend 'layout.html'}}
<h1>This is the default/uploadresume.html template</h1>
{{=BEAUTIFY(response._vars)}}
    <div class="form" id="JSFiddle">
        <style>
            #JSFiddle div.block p {
                line-height: 1.4em;
                margin: 0 0 .6em
            }

            #JSFiddle div.block p.sub-title {
                font-size: 1.2em;
                font-weight: bold;
                margin: 0
            }

            #JSFiddle div.block p.sub-where {
                font-size: 1.1em;
                margin: 0
            }

            #JSFiddle div.block p.sub-dates {
                font-size: 1em;
                color: #555;
                margin: 0 0 .5em
            }

            #JSFiddle div.block p.person {
                font-size: 4em;
                margin-bottom: 0;
                line-height: 1.2em;
                margin-top: -.2em;
            }

            #JSFiddle div.block p.jobtitle {
                font-size: 1.2em;
                margin: 0
            }

            #JSFiddle div.block p.location {
                font-size: 1em;
                color: #555;
                margin: 0
            }

            #JSFiddle div.block span.company {
                color: #555;
                font-weight: bold
            }

            #JSFiddle div.block p.title {
                font-size: 1.3em;
                font-weight: bold;
                margin: 0 0 .5em 0;
                padding: 0 .5em .1em;
                text-transform: uppercase;
                background: #ddd;
                border: 1px solid #ddd;
                -moz-border-radius: 0 0 1.3em 0;
                -webkit-border-radius: 0 0 1.3em 0;
                border-radius: 0 0 1.3em 0;
                border-bottom-right-radius: 1.3em
            }

            #JSFiddle div.blocks {
                margin-top: 1em
            }

            #JSFiddle div.block-inner {
                margin-left: 2em
            }

            #JSFiddle td.block-title {
                width: 50%;
                padding: 0;
                border-top: .2em solid #ddd
            }

            #JSFiddle table.wide {
                width: 100%;
                border-collapse: collapse
            }

            #JSFiddle table.wide.title {
                page-break-inside: avoid;
                page-break-after: avoid
            }

            #JSFiddle td.left {
                width: 65%;
                vertical-align: top
            }

            #JSFiddle td.right {
                width: 35%;
                vertical-align: top;
                text-align: right
            }
        </style>
        <table class="wide">
            <tbody>
                <tr>
                    <td class="left">
                        <div class="block person" data-category="person" style="font-family: 'Roboto Condensed'; margin-bottom: 1em;">
                            <p class="person" id="chname">First Name Last Name</p>
                            <p class="jobtitle">Most Recent Job Title</p>
                            <p class="location">Location</p>
                        </div>
                    </td>
                    <td class="right">
                        <div class="block contact" data-category="contact" style="margin-bottom: 1em;">
                            <div class="html-content" id = "chemail">your.name@example.com
                                <br>111-222-3333
                                <br>www.your-website.com</div>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
        <div class="blocks">
            <div class="block" data-category="text" data-children="" data-id="0" style="margin-bottom: 1em;">
                <table class="wide title" cellspacing="0">
                    <tbody>
                        <tr>
                            <td class="block-title">
                                <p class="title" style="font-family: 'Roboto Condensed';">Summary</p>
                            </td>
                            <td class="block-title"></td>
                        </tr>
                    </tbody>
                </table>
                <div class="block-inner">
                    <div class="html-content">List here your top selling points, including your most relevant strengths, skills and core competencies.</div>
                </div>
            </div>
            <div class="block" data-category="experience" data-children="2" data-id="1" style="margin-bottom: 1em;">
                <table class="wide title" cellspacing="0">
                    <tbody>
                        <tr>
                            <td class="block-title">
                                <p class="title" style="font-family: 'Roboto Condensed';">Work Experience</p>
                            </td>
                            <td class="block-title"></td>
                        </tr>
                    </tbody>
                </table>
                <div class="block-inner">
                    <div class="child" data-id="1c0" data-category="experience" style="margin-bottom: 1em;">
                        <p class="sub-title" style="font-family: 'Roboto Condensed';">Job Title</p>
                        <p class="sub-where"><span class="company">Company Name</span>, Location</p>
                        <p class="sub-dates">Jan 2013 - Dec 2013</p>
                        <div class="html-content">Describe your job responsibilities, accomplishments and technologies you have used. It is highly recommended that you use bullet points to describe your experience.</div>
                    </div>
                    <div class="last-child" data-id="1c1" data-category="experience" style="margin-bottom: 1em;">
                        <p class="sub-title" style="font-family: 'Roboto Condensed';">Job Title</p>
                        <p class="sub-where"><span class="company">Company Name</span>, Location</p>
                        <p class="sub-dates">Jan 2014 - Dec 2014</p>
                        <div class="html-content">Describe your job responsibilities, accomplishments and technologies you have used. It is highly recommended that you use bullet points to describe your experience.</div>
                    </div>
                </div>
            </div>
            <div class="block" data-category="education" data-children="1" data-id="2" style="margin-bottom: 1em;">
                <table class="wide title" cellspacing="0">
                    <tbody>
                        <tr>
                            <td class="block-title">
                                <p class="title" style="font-family: 'Roboto Condensed';">Education</p>
                            </td>
                            <td class="block-title"></td>
                        </tr>
                    </tbody>
                </table>
                <div class="block-inner">
                    <div class="last-child" data-id="2c0" data-category="education" style="margin-bottom: 1em;">
                        <p class="sub-title" style="font-family: 'Roboto Condensed';">Degree</p>
                        <p class="sub-where">School Name, Location</p>
                        <p class="sub-dates">2013 - 2013</p>
                        <div class="html-content">(Optional) GPA, Awards, Honors</div>
                    </div>
                </div>
            </div>
            <div class="block" data-category="text" data-children="" data-id="3" style="margin-bottom: 1em;">
                <table class="wide title" cellspacing="0">
                    <tbody>
                        <tr>
                            <td class="block-title">
                                <p class="title" style="font-family: 'Roboto Condensed';">Additional Information</p>
                            </td>
                            <td class="block-title"></td>
                        </tr>
                    </tbody>
                </table>
                <div class="block-inner">
                    <div class="html-content">Include other relevant information that employers should know about. This may include activities, experiences and interests that you have that relate to the position you are trying to get.</div>
                </div>
            </div>
        </div>
    </div>
<form class="forname">
        <input id="chname" type="text" onkeyup="changename(this)">
    </form>
        <form class="foremail">
        <input id="chemail" type="text" onkeyup="changeemail(this)">
    </form>
       <script>
           document.getElementById('chname').value=document.getElementById('nam').innerHTML;
           document.getElementById('chemail').value=document.getElementById('emai').innerHTML;
           function changename(it)
           {
               var a = document.getElementById('chname').value;
               if (a)
               {
                   document.getElementById('nam').innerHTML=a;
               }
               else 
               {
                    document.getElementById('nam').innerHTML="Name Not Provided";
               }
           }
           function changeemail(it)
           {
               var a = document.getElementById('chemail').value;
               if (a)
               {
                   document.getElementById('emai').innerHTML=a;
               }
               else 
               {
                    document.getElementById('emai').innerHTML="Email Not Provided";
               }
           }
       </script>
