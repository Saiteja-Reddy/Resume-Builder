<script>
    
    var abcd=0;
    var a1=0;var a2=0;var a3=0;var a4=0;var a5=0;var a6=0;var a7=0;var a8=0;var a9=0;
    
    document.getElementById('cheph').value = document.getElementById('ph').innerHTML;
    document.getElementById('chname').value = document.getElementById('nam').innerHTML;
    document.getElementById('chemail').value = document.getElementById('emai').innerHTML;
    document.getElementById('chesumm').value = document.getElementById('su1').innerHTML;
    document.getElementById('chewo').value = document.getElementById('wo1').innerHTML;
    document.getElementById('chewo2').value = document.getElementById('wo2').innerHTML;
    document.getElementById('cheed').value = document.getElementById('ed1').innerHTML;
    document.getElementById('chead').value = document.getElementById('ad1').innerHTML;
    document.getElementById('cheed2').value = document.getElementById('ed2').innerHTML;

    function changes(str1, str2) {
        var a = document.getElementById(str1).value;
        if (a) {
            document.getElementById(str2).innerHTML = a;
        } else {
            document.getElementById(str2).innerHTML = "Not Provided";
        }
        if(str1=='cheph'){ if(!a1) { a1=1 ,abcd+=1; } }
        if(str1=='chname'){ if(!a2) { a2=1 ,abcd+=1; } }
        if(str1=='chemail'){ if(!a3) { a3=1 ,abcd+=1; } }
        if(str1=='chesum'){ if(!a4) { a4=1 ,abcd+=1; } }
        if(str1=='chewo'){ if(!a5) { a5=1 ,abcd+=1; } }
        if(str1=='chewo2'){ if(!a6) { a6=1 ,abcd+=1; } }
        if(str1=='cheed'){ if(!a7) { a7=1 ,abcd+=1; } }
        if(str1=='chead'){ if(!a8) { a8=1 ,abcd+=1; } }
        if(str1=='cheed2'){ if(!a9) { a9=1 ,abcd+=1; } }
        prog(abcd);
    }
    function prog(abc)
    {
        switch (abc) {
    case 0:
           document.getElementById('bar').style.width='0%';
           document.getElementById('bar').innerHTML="0%";
        break;
    case 1:
                document.getElementById('bar').style.width='10%';
           document.getElementById('bar').innerHTML="10%";
                break;
    case 2:
             document.getElementById('bar').style.width='20%';
             document.getElementById('bar').innerHTML="20%";
        break;
    case 3:
             document.getElementById('bar').style.width='30%';
             document.getElementById('bar').innerHTML="30%";
        break;
    case 4:
             document.getElementById('bar').style.width='40%';
             document.getElementById('bar').innerHTML="40%";
        break;
    case 5:
             document.getElementById('bar').style.width='50%';
             document.getElementById('bar').innerHTML="50%";
        break;
    case 6:
             document.getElementById('bar').style.width='60%';
             document.getElementById('bar').innerHTML="60%";
        break;
    case 7:
             document.getElementById('bar').style.width='70%';
             document.getElementById('bar').innerHTML="70%";
        break;
    case 8:
             document.getElementById('bar').style.width='100%';
             document.getElementById('bar').innerHTML="100%";
        break;
    }
    }
    function printDiv(divName) {
        window.print();
    }
</script>
