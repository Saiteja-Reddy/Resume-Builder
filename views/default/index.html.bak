{{left_sidebar_enabled,right_sidebar_enabled=False,('message' in globals())}}
{{extend 'layout.html'}}

{{block header}}
    <header class="container-fluid background">
      <div class="jumbotron text-center">
        {{if response.title:}}
        <h1>{{=response.title}}
          <small>{{=response.subtitle or ''}}</small></h1>
        {{pass}}
      </div>
    </header>
{{end}}
<a href={{=URL('default','templates')}}>Fill a resume</a>
<br>
<a href={{=URL('default','findresume')}}>Search for a resume</a>
<br>
<a href={{=URL('default','myresume')}}>My Resumes</a>
<br>
<a href={{=URL('default','markedresume')}}>Marked Resumes</a>
<br>
<a href={{=URL('default','allresume')}}>All Resumes</a>
<br>

{{if 'message' in globals():}}
<h2>{{=message}}</h2>
{{elif 'content' in globals():}}
{{=content}}
{{else:}}
{{=BEAUTIFY(response._vars)}}
{{pass}}

{{block right_sidebar}}
{{end}}
